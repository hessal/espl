
CC = gcc
    CFLAGS = -g

%.o: %.asm
	nasm -g -f elf -o $@ $<
.PHONY: clean all

all: ascart2

ascart2: ascart2.o write.o returnStrlen.o font.o
	$(CC) -m32 $(LDFLAGS) -o $@ $^
	
ascart2.o: ascart2.c 
	$(CC) -m32 $(CFLAGS) -c -o $@ $<

font.o: font.c font.h
	$(CC) -m32 $(CFLAGS) -c -o $@ $<


clean:
	rm -f ascart2 ascart2.o 

